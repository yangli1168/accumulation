<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="model.StudentCourseMapper">
 	<insert id="add" parameterType="model.StudentCourseMapper">
 		insert into student_course (sid,cid) values(#{student.id},#{course.id})
 	</insert>
 	
 	<delete id="delete">
 		delete from student_course where sid=#{student.id} and cid=#{course.id}
 	</delete>
 	
</mapper>